{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "environment": "client",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_source}",
    "issues": "${mod_issues}"
  },
  "license": "${mod_license}",
  "icon": "icon.png",
  "entrypoints": {
    "client": ["net.flytre.salutem.Salutem"]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "accessWidener": "${mod_id}.accessWidener",
  "depends": {
    "fabricloader": ">=0.10.8",
    "minecraft": "1.19.x",
    "flytre_lib": ">=${lib_version}"
  }
}
